border a1(t=0,1){x=t;y=0;label=1;}
border a2(t=0,1){x=1;y=0.5*t;label=1;}
border a3(t=0,1){x=1-0.5*t;y=0.5;label=1;}
border a4(t=0,1){x=0.5;y=0.5+0.5*t;label=1;}
border a5(t=0,1){x=0.5-0.5*t;y=1;label=1;}
border a6(t=0,1){x=0;y=1-t;label=1;}
int n=25;
mesh Th = buildmesh(a1(4)+a2(2)+a3(2)+a4(2)+a5(2)+a6(4));
plot(Th);


